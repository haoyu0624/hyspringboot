        <version>4.5.3</version>
      </dependency>
      <dependency>
        <groupId>com.github.rholder</groupId>
        <artifactId>guava-retrying</artifactId>
        <version>2.0.0</version>
      </dependency>
      <dependency>
        <groupId>com.glodon.kafka.api.internal</groupId>
        <artifactId>glodon-kafka-api</artifactId>
